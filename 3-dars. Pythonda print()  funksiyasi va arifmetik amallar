# Arifmetik amallar
print(2*5)
print(4*9)

print(0-3)
print(9-4)

print(9+25)
print(11+89)

print(8/5)
print(2/1)

print(2+5+6)
print(7+9*2)
print(8*2*1+8-14/2)

print(25/6)
print(25//6)

print(2**2)
print(2**9)
print(3**2)
print(5**4)

print("9 ni 9 ga ko'paytirak natija", 9*9, "ga teng")
print("8 ni 7 ga qo'shsak natija", 8+7, "ga teng")

